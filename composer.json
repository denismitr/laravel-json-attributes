{
    "name": "denismitr/laravel-json-attributes",
    "description": "Laravel Json Attributes for Eloquent Models",
    "keywords": ["denismitr", "laravel", "eloquent", "json", "json-attributes"],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Mitrofanov",
            "email": "denis.mitr@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/database": "~5.7.0|~5.8.0",
        "illuminate/support": "~5.7.0|~5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "orchestra/testbench": "~3.7.0|~3.8.0",
        "larapack/dd": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Denismitr\\JsonAttributes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Denismitr\\JsonAttributes\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./test.sh",
        "test-coverage": "vendor\\bin\\phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Denismitr\\JsonAttributes\\JsonAttributesServiceProvider"
            ]
        }
    }
}
